#include<bits/stdc++.h>
using namespace std;
 bool compare(pair<int,int>p1, pair<int,int>p2){
   return p1.first<p2.first;
 }
 bool compare1(pair<int,int>p1,int n){
     return p1.first<n;
 }
 vector<int> check(vector<pair<int,int>> v,int*arr,int m){
    vector<int> v1;
    for(int i=0;i<m;i++){
        vector<pair<int,int>> :: iterator it;
        it=lower_bound(v.begin(),v.begin()+v.size(),arr[i],compare1);
        if(it->first==arr[i]){
            v1.push_back(0);
            continue;
        }
        if(it>=v.end()){
            v1.push_back(-1);
        }
        if(it->first>arr[i]){
            if((it-1)<v.begin() || (it-1)->second==arr[i]){
                v1.push_back(it->first-arr[i]);
            }else{
              v1.push_back(0);
            }
        }else{
         v1.push_back(-1);
        }
    }
    return v1;
 }
int main(){
 int n,m;
 cin>>n>>m;
 vector<pair<int,int>> v;
 for(int i=0;i<n;i++){
    pair<int,int> p;
    cin >>p.first>>p.second;
    v.push_back(p);
 }
 int arr[m];
 for(int i=0;i<m;i++){
    cin>>arr[i];
 }

 sort(v.begin(),v.begin()+n,compare);
 vector<int> v1=check(v,arr,m);
 for(int i=0;i<v1.size();i++){
    cout<<v1.at(i)<<" ";
 }
}
